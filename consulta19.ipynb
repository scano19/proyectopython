    "#lista de numeros\n",
    "lista_numeros = [5, 7, 2, 4]\n",
    "#una funci√≥n con filter para los numeros impares\n",
    "def numeros_impares(lista_numeros):\n",
    "    return list(filter(lambda x: x % 2 != 0, lista_numeros))\n",
    "print (numeros_impares(lista_numeros))"
