    "#calcular la diferencia de los valores de dos listas\n",
    "#uso la funci√≥n map()\n",
    "def calcular_diferencia_map(lista1, lista2):\n",
    "    return list(map(lambda a, b: a - b, lista1, lista2))\n",
    "lista1 = [10, 20, 30]\n",
    "lista2 = [1, 2, 3]\n",
    "print(calcular_diferencia_map(lista1, lista2))\n",
    "\n"
   ]
